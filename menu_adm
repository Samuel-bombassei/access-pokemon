Option Compare Database

Private Sub btn_cad_contas_Click()
DoCmd.Close
DoCmd.OpenForm ("Cad_contas"), acNormal
End Sub

Private Sub btn_encerrar_Click()
    DoCmd.Close
    DoCmd.OpenForm "login"
End Sub

Private Sub cmb_cad_cartas_Click()
DoCmd.Close
DoCmd.OpenForm ("cadastro_carta"), acNormal
End Sub

